{
    "name": "qinp-lang",
    "displayName": "QINP Lang",
    "description": "Syntax highlighting and snippets for QINP",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.66.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/eQosys/qinp-lang.git"
    },
    "publisher": "TecStylos",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "qinp",
            "aliases": ["QINP", "qinp"],
            "extensions": [".qnp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "qinp",
            "scopeName": "source.qnp",
            "path": "./syntaxes/qinp.tmLanguage.json"
        }],
        "snippets": [{
            "language": "qinp",
            "path": "./snippets/qinp.json"
        }]
    }
}